restriction Equality:
  "∀ x y #i. (Eq( x, y ) @ #i) ⇒ (x = y)"
  // safety formula

restriction Inequality:
  "∀ x #i. (Neq( x, x ) @ #i) ⇒ (⊥)"
  // safety formula

restriction Unique:
  "∀ x #i #j. ((Unique( x ) @ #i) ∧ (Unique( x ) @ #j)) ⇒ (#i = #j)"
  // safety formula

restriction OnlyOneNP:
  "∀ x y #i #j.
    ((InitializeNP( x ) @ #i) ∧ (InitializeNP( y ) @ #j)) ⇒ (#i = #j)"
  // safety formula

restriction AdversaryDeviceCompromise:
  "∀ D #i.
    (RequireOSCompromise( D ) @ #i) ⇒
    (∃ #j. (OSCompromise( D ) @ #j) ∧ (#j < #i))"

restriction OnlyOneAggressor:
  "∀ AGGR AGGR2 #i #j.
    ((AggressorInitialized( AGGR ) @ #i) ∧
     (AggressorInitialized( AGGR2 ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction AdversaryDoesNotExtractPropertiesFromOwnAgents:
  "∀ A prop #i #j.
    ((AdversaryExtractAgentProperties( A, prop ) @ #i) ∧
     (AdversaryCreateAgent( A, prop ) @ #j)) ⇒
    (⊥)"
  // safety formula

restriction OTEESessionCannotInterleaveWithSameNonce:
  "∀ OTEE u n1 #i #j.
    ((OTEESessionAggrNonce( OTEE, u, n1 ) @ #i) ∧
     (OTEESessionAggrNonce( OTEE, u, n1 ) @ #j)) ⇒
    (∀ u_2 #k.
      ((((OTEESessionAggrNonce( OTEE, u_2, n1 ) @ #k) ∧ (#i < #k)) ∧
        (#k < #j)) ∧
       (¬(u = u_2))) ⇒
      (⊥))"
  // safety formula

restriction OTEESessionHasToFinishBeforeStartingNext:
  "∀ OTEE u u_2 n_1 #i #j.
    ((((OTEESessionAggrNonce( OTEE, u, n_1 ) @ #i) ∧
       (OTEESessionAggrNonce( OTEE, u_2, n_1 ) @ #j)) ∧
      (#i < #j)) ∧
     (¬(u = u_2))) ⇒
    (∃ C #k.
      ((OTEEChallengeResponse( OTEE, C, u ) @ #k) ∧ (#i < #k)) ∧ (#k < #j))"

