lemma adversaryCannotRecoverServerDHSecret [reuse]:
  all-traces
  "∀ AGGR x_0 x_1 X_1 #i #j.
    ((AggrDH( AGGR, x_0, x_1, X_1 ) @ #i) ∧ (!KU( x_1 ) @ #j)) ⇒ (⊥)"
/*
guarded formula characterizing all counter-examples:
"∃ AGGR x_0 x_1 X_1 #i #j.
  (AggrDH( AGGR, x_0, x_1, X_1 ) @ #i) ∧ (!KU( x_1 ) @ #j)"
*/
simplify
by solve( !KU( ~x_1 ) @ #j )

lemma adversaryCannotFindPropertiesFromAggressor [reuse]:
  all-traces
  "∀ OTEE ch_pos ch_prop A #i #j #k.
    (((OTEEMeasure( OTEE, ch_pos, ch_prop ) @ #i) ∧
      (OUT_Aggressor_0( ch_prop ) @ #j)) ∧
     (AdversaryCreateAgent( A, ch_prop ) @ #k)) ⇒
    (⊥)"
/*
guarded formula characterizing all counter-examples:
"∃ OTEE ch_pos ch_prop A #i #j #k.
  (OTEEMeasure( OTEE, ch_pos, ch_prop ) @ #i) ∧
  (OUT_Aggressor_0( ch_prop ) @ #j) ∧
  (AdversaryCreateAgent( A, ch_prop ) @ #k)"
*/
simplify
solve( Aggressor_0( $AGGR, ~prop, $A, ~ltk ) ▶₀ #j )
  case Aggressor_Create_Agent
  by solve( !KU( ~prop ) @ #vk.4 )
qed

